{
  "name": "react-ts-template",
  "version": "1.0.0",
  "description": "For kickstarting simple apps on the fly",
  "main": "index.js",
  "engines": {
    "node": "10.11.0"
  },
  "scripts": {
    "start": "npm run build-static && webpack-dev-server --mode development",
    "clean": "rm -rf dist",
    "build-static": "shx mkdir -p dist/ && cp -r public/* dist/",
    "serve": "npm run build && http-server ./dist -p 8000",
    "build": "npm run build-static && webpack --mode production",
    "prettify": "prettier --write src/"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "Julius Rajala <juliusrajala@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "file-loader": "6.2.0",
    "http-server": "~0.12.1",
    "prettier": "~2.0.2",
    "redux-devtools-extension": "^2.13.8",
    "shx": "^0.2.2",
    "source-map-loader": "^4.0.1",
    "ts-loader": "9.4.2",
    "typescript": "^2.9.2",
    "webpack": "~5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": "^3.4.10"
  }
}
